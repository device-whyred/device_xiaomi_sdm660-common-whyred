allow untrusted_app theme_prop:file { read };
allow untrusted_app persist_camera_prop:file { open read getattr };
allow untrusted_app sysfs_kgsl:dir { search };
allow untrusted_app sysfs_kgsl:lnk_file { read open getattr };
get_prop(untrusted_app, persist_camera_prop)  
get_prop(untrusted_app, camera_prop)  

allow untrusted_app vendor_default_prop:file { getattr };
allow untrusted_app app_data_file:file { execute };
allow untrusted_app vendor_default_prop:file { read open };
allow untrusted_app proc_qtaguid_stat:file { read }; 
allow untrusted_app proc_zoneinfo:file { read };
allow untrusted_app rootfs:dir { read };

dontaudit untrusted_app sysfs_kgsl:lnk_file read;
dontaudit untrusted_app proc_vmstat:file { getattr open };
